{
  "defaultEnv": "development",
  "development": {
    "driver": "pg",
    "host": "localhost",
    "port": "${process.env.SQL_PORT}",
    "database": "${process.env.SQL_DATABASE}",
    "user": "${process.env.SQL_USER}",
    "password": "${process.env.SQL_PASSWORD}",
    "schema": "public",
    "ssl": {
      "rejectUnauthorized": false
    }
  },
  "production": {
    "driver": "pg",
    "host": "${process.env.SQL_HOST}",
    "port": "${process.env.SQL_PORT}",
    "database": "${process.env.SQL_DATABASE}",
    "user": "${process.env.SQL_USER}",
    "password": "${process.env.SQL_PASSWORD}",
    "schema": "public",
    "ssl": {
      "rejectUnauthorized": false
    }
  }
}
